{
  "name": "LinkedIn Connect",
  "description": "Auto connect tool",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  }
}